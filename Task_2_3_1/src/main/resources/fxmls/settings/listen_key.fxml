<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.stage.*?>

<Stage fx:id="listenKeyStage" alwaysOnTop="true" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nsu.fit.oop.melnikov.game.snake.presenter.presenters.settings.ListenKeyPresenter">
    <scene>
        <Scene fx:id="listenKeyScene">
            <VBox alignment="TOP_CENTER">
                <Label text="Press a key...">
                    <font>
                        <Font size="39.0" />
                    </font>
                    <VBox.margin>
                        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
                    </VBox.margin>
                </Label>
                <Button mnemonicParsing="false" onAction="#onCancelClick" text="Cancel">
                    <font>
                        <Font size="19.0" />
                    </font>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
                </Button>
            </VBox>
        </Scene>
    </scene>
</Stage>
